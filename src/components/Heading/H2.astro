---
import type { HTMLAttributes } from 'astro/types';

interface Props extends HTMLAttributes<'h2'> { 
    /**
     * @prop class? : string
     */
    props? : true
    class? : string,
}

const { class: className, ...rest } = Astro.props
---

<h2 class:list={['text-2xl font-bold', className]} {...rest}>
    <slot />
</h2>